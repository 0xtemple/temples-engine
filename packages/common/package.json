{
  "name": "@0xtempl/common",
  "version": "0.5.6",
  "description": "Common low level logic shared between packages",
  "keywords": [
    "sui",
    "templs labs",
    "move",
    "blockchain"
  ],
  "type": "module",
  "author": "team@templs.build",
  "homepage": "https://github.com/0xtempl/templs-engine/tree/main/packages/common#readme",
  "bugs": "https://github.com/0xtempl/templs-engine/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xtempl/templs-engine.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ]
    }
  },
  "types": "src/index.ts",
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup && chmod +x ./dist/index.js",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "execa": "^7.0.0",
    "find-up": "^6.3.0",
    "glob": "^8.0.3",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "prettier-plugin-rust": "^0.1.9",
    "typescript": "5.1.6",
    "yargs": "^17.7.1",
    "zod": "^3.22.3",
    "zod-validation-error": "^1.3.0",
    "esbuild": "^0.17.15"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.15.11",
    "@types/yargs": "^17.0.10",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6"
  }
}
